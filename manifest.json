{
  "manifest_version": 3,
  "name": "History Analytics",
  "version": "1.0",
  "description": "AI-powered browsing history analytics",
  "permissions": [
    "history",
    "storage"
  ],
  "chrome_url_overrides": {
    "history": "index.html"
  },
  "background": {
    "service_worker": "src/background.ts"
  },
  "host_permissions": [
    "*://*/*"
  ]
}